{
    "name": "sparallel/laravel",
    "description": "Parallel PHP via processes for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sprust28@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-redis": "*",
        "laravel/framework": "^9.0 | ^10.0 | ^11.0",
        "sparallel/sparallel": "dev-master",
        "ext-posix": "*",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "orchestra/testbench": "^9.13"
    },
    "autoload": {
        "psr-4": {
            "SParallelLaravel\\": "src/SParallelLaravel"
        }
    },
    "bin": [
        "bin/sparallel-process",
        "bin/sparallel-hybrid-process"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SParallelLaravel\\SParallelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "SParallelLaravel\\Tests\\": "tests/feature"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    }
}
