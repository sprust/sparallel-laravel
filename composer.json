{
    "name": "sparallel/sparallel-laravel",
    "description": "Parallel PHP via processes for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sprust28@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0 | ^10.0 | ^11.0",
        "sparallel/sparallel": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "SParallelLaravel\\": "src/SParallelLaravel"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SParallelLaravel\\SParallelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
